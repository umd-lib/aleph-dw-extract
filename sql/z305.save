 || Z305_DELINQ_N_1 || '	'
 || TO_CHAR(NVL(Z305_DELINQ_1_UPDATE_DATE,0),'FM00000000')  || '	'
 || Z305_DELINQ_1_CAT_NAME || '	'
 || TO_CHAR(NVL(Z305_DELINQ_2,0),'FM00')  || '	'
 || Z305_DELINQ_N_2 || '	'
 || TO_CHAR(NVL(Z305_DELINQ_2_UPDATE_DATE,0),'FM00000000')  || '	'
 || Z305_DELINQ_2_CAT_NAME || '	'
 || TO_CHAR(NVL(Z305_DELINQ_3,0),'FM00')  || '	'
 || Z305_DELINQ_N_3 || '	'
 || TO_CHAR(NVL(Z305_DELINQ_3_UPDATE_DATE,0),'FM00000000')  || '	'
 || Z305_DELINQ_3_CAT_NAME || '	'
 || Z305_FIELD_1 || '	'
 || Z305_FIELD_2 || '	'
 || Z305_FIELD_3 || '	'
 || Z305_HOLD_ON_SHELF || '	'
