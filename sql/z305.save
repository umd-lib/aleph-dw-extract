 || TO_CHAR(NVL(Z305_NO_LOAN,0),'FM0000')  || '	'
 || TO_CHAR(NVL(Z305_NO_HOLD,0),'FM0000')  || '	'
 || TO_CHAR(NVL(Z305_NO_PHOTO,0),'FM0000')  || '	'
 || TO_CHAR(NVL(Z305_NO_CASH,0),'FM0000')  || '	'
 || Z305_CASH_LIMIT || '	'
 || Z305_CREDIT_DEBIT || '	'
 || Z305_SUM || '	'
