 || Z305_BOOKING_PERMISSION || '	'
 || Z305_BOOKING_IGNORE_HOURS || '	'
 || Z305_RUSH_CAT_REQUEST || '	'
 || Z305_UPD_TIME_STAMP || '	'
 FROM Z305
WHERE Z305_REC_KEY = ?
